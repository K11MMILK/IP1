<Page x:Class="ЗИ_лаба1.UserPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ЗИ_лаба1"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UserPage"
      Background="White">

    <Grid>
        <Button FontSize="14" Content="Назад" x:Name="backBtn" Click="backBtn_click" Margin="10,10,10,10" Height="25" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
        <Button FontSize="14" Content="Изменить пароль" x:Name="passChangeBtn" Click="passChangeBtn_click" Margin="10,10,10,10" Height="25" Width="150" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Button FontSize="14" Content="Список пользователей" x:Name="peopleCheckListBtn" Click="peopleCheckListBtn_click" Margin="10,45,10,10" Height="25" Width="150" VerticalAlignment="Top" HorizontalAlignment="Left"/>
    </Grid>
</Page>
